{
   "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "skipLibCheck": true,
      "strict": false,
      "allowJs": true,
      "noEmit": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "types": [
         "@types/node"
      ],
      "paths": {
         "#app": [
            "./node_modules/nuxt3/dist/app"
         ],
      }
   },
   "include": [
      "**/*"
   ]
}